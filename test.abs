token = env("GH_TOKEN")
no_more_repos = false
page = 1

while !no_more_repos {
    repos = `curl -s -H "Authorization: token $token" "https://api.github.com/orgs/namshi/repos?page=$page&type=private"`.json()

    if !repos.len() {
        no_more_repos = true
        echo("End of repositories!")
        return null
    }

    for r in repos {
        repo_name = r.full_name
        echo("Processing %s...", repo_name)
    }

    page += 1
}
